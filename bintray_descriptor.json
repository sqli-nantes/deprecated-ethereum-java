{
  "package": {
    "name": "ethereum-java",
    "repo": "ethereum-java",
    "subject": "sqli-nantes",
    "desc": "I was pushed automatically from Travis-CI",
    "vcs_url": "https://github.com/sqli-nantes/ethereum-java/",
    "github_use_tag_release_notes": true,
    "github_release_notes_file": "RELEASE.txt",
    "licenses": ["MIT"],
    "labels": ["blockchain", "ethereum", "java"],
    "public_download_numbers": false,
    "public_stats": false
  },

  "version": {
    "name": "${project.version}",
    "released": "${build.version}",
    "vcs_tag": "${project.version}",
    "gpgSign": false
  },

  "files":
  [
    {"includePattern": "ethereum-java-core/build/libs/(.*)", "uploadPattern": "/ethereum-java-core/snapshots/$1"},
    {"includePattern": "ethereum-java-sample/build/libs/(.*)", "uploadPattern": "/ethereum-java-sample/snapshots/$1"}
  ],
  "publish": true
}